!include include/default_mysqld.cnf

[mysqld.1]
early-plugin-load= @ENV.FILE_KEY_MANAGEMENT_SO
loose-keyring_file_data= /home/rob/keys1.txt
gtid-mode=ON
enforce-gtid-consistency=ON
encrypt-binlog=1
master-verify-checksum=1

[mysqld.2]
early-plugin-load= @ENV.FILE_KEY_MANAGEMENT_SO
loose-keyring_file_data= /home/rob/keys2.txt
log-slave-updates
relay-log-info-repository=table
gtid-mode=ON
enforce-gtid-consistency=ON
encrypt-binlog=1
master-verify-checksum=1

[mysqld.3]
early-plugin-load= @ENV.FILE_KEY_MANAGEMENT_SO
loose-keyring_file_data= /home/rob/keys3.txt
log-slave-updates
relay-log-info-repository=table
gtid-mode=ON
enforce-gtid-consistency=ON
encrypt-binlog=1
master-verify-checksum=1

[ENV]
SERVER_MYPORT_1= @mysqld.1.port
SERVER_MYPORT_2= @mysqld.2.port
SERVER_MYPORT_3= @mysqld.3.port
